{
    "name": "dn_what3words",
    "version": "1.0.5-SNAPSHOT",
    "title": "what3words",
    "description": "what3words",
    "productName": "devnet-mapapps-what3words",
    "main": "",
    "i18n": [
        "bundle"
    ],
    "dependencies": {
        "esri": "^4.10.1",
        "apprt-core": "^4.6.2",
        "apprt-binding": "^4.6.2",
        "apprt-request": "^4.6.2",
        "ct": "^4.6.2",
        "store-api": "^4.6.2"
    },
    "cssThemesExtension": [
        {
            "name": "*",
            "files": [
                "styles/styles.css"
            ]
        }
    ],
    "components": [
        {
            "name": "Config",
            "provides": "dn_what3words.What3WordsModel",
            "propertiesConstructor": true,
            "properties": {
                "apiKey": "",
                "what3wordsUrl": "https://api.what3words.com/v3/convert-to-3wa"
            }
        },
        {
            "name": "What3WordsStoreFactory",
            "properties": {
                "useIn": [
                    "search"
                ],
                "omniSearchTypingDelay": 200,
                "omniSearchPageSize": 20,
                "omniSearchSearchAttr": "title",
                "omniSearchDefaultLabel": "///",
                "omniSearchPriority": 9999,
                "omniSearchAutoActivate": true,
                "-filterQuery": {},
                "-filterOptions": {
                    "suggestContains": true
                }
            },
            "references": [
                {
                    "name": "_model",
                    "providing": "dn_what3words.What3WordsModel"
                }
            ]
        },
        {
            "name": "What3WordsPopupAction",
            "provides": "map-actions.Action",
            "immediate": true,
            "properties": {},
            "references": [
                {
                    "name": "_mapWidgetModel",
                    "providing": "map-widget.MapWidgetModel"
                },
                {
                    "name": "_actionService",
                    "providing": "map-actions.ActionService"
                },
                {
                    "name": "_what3WordsModel",
                    "providing": "dn_what3words.What3WordsModel"
                },
                {
                    "name": "_what3WordsStore",
                    "providing": "ct.api.Store",
                    "filter": "(id='what3wordsStore')",
                    "cardinality": "0..1"
                }
            ]
        },
        {
            "name": "PopupToggleTool",
            "impl": "ct/tools/Tool",
            "provides": [
                "ct.tools.Tool"
            ],
            "propertiesConstructor": true,
            "properties": {
                "id": "popupToggleTool",
                "title": "${tool.title}",
                "tooltip": "${tool.tooltip}",
                "toolRole": "toolset",
                "iconClass": "",
                "togglable": true,
                "activateHandler": "activateTool",
                "deactivateHandler": "deactivateTool"
            },
            "references": [
                {
                    "name": "handlerScope",
                    "providing": "dn_what3words.MapClickPopupHandler"
                }
            ]
        },
        {
            "name": "MapClickPopupHandler",
            "provides": [
                "dn_what3words.MapClickPopupHandler"
            ],
            "propertiesConstructor": true,
            "references": [
                {
                    "name": "_mapWidgetModel",
                    "providing": "map-widget.MapWidgetModel"
                },
                {
                    "name": "_what3WordsModel",
                    "providing": "dn_what3words.What3WordsModel"
                }
            ]
        },
        {
            "name": "PopupActionCopyWhat3WordsFactory",
            "provides": [
                "popups.ActionFactory"
            ]
        }
    ]
}
