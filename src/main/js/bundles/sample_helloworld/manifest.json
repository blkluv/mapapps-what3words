{
    "name": "sample_helloworld",
    "version": "4.6.1-SNAPSHOT",
    "title": "${bundleName}",
    "description": "${bundleDescription}",
    "vendor": "con terra GmbH",
    "keywords": [
        "sample"
    ],
    "icon": {},
